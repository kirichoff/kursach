
        Insert
        into
             UserShop
            (login,password,email,phoneNumber)
        values
            ('${login}','${password}','${email}',${phoneNumber});
           select lastval() as userId;

        Insert
        into
             ShopItem
            (description,header,categoryId,price)
        values
            ('${description}','${header}',${categoryId},${price});
                select lastval() as Id;

